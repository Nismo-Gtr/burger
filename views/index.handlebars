<h1>Eat Da Burger</h1>

<ul>
  {{#each burgers}}
    {{#unless devoured}}
      {{> burgers devoured=false}}
    {{/unless}}
  {{/each}}
</ul>

<ul>
  {{#each burgers}}
    {{#if devoured}}
      {{> burgers devoured=true}}
    {{/if}}
  {{/each}}
</ul>


<form class="create-form">

  <div class="form-group">
    <input type="text" id="createBurger" name="name">
  </div>
    <button type="submit">Add Burger</button>
</form>
